<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item
    android:left="0dp"
       >
        <shape
            android:shape="rectangle">

            <solid android:color="@color/white" />

            <stroke
                android:width="1dp"
                android:color="@color/intro_skip" />
            <corners android:radius="4dp" />
            <padding
                android:top="5dp"
                android:bottom="5dp"/>
        </shape>
    </item>

</layer-list>

